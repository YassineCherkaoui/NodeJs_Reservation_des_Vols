    <!-- header-end -->

    <!-- bradcam_area  -->
    <div class="bradcam_area bradcam_bg_2">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="bradcam_text text-center">
                        <h3>Destinations</h3>
                        <p>Pixel perfect design with awesome contents</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/ bradcam_area  -->

    <div class="destination_details_info">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8 col-md-9">
                    <div class="destination_info">
                    <div class="contact_join">
                        <h3>Réservation</h3>
                        <form action="/reservation/<%= id_airplane %>" method="POST">
                            <div class="row">
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">

                                        <input name="id_airplane" type="hidden" class="form-control date" id="id_airplane" value="<%= id_airplane %>">
                                        <input type="text" placeholder="YOUR CODE" name="code">
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        <input type="text" placeholder="Type FIRST NAME..." name="nom">
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        <input type="text" placeholder="Type LAST NAME..." name="prénom">
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        <input type="email" placeholder="Type Email..." name="email">
                                    </div>
                                </div>                             
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        <input type="tel" placeholder="Phone no." name="téléphone">
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        <input type="text" placeholder="Type Passport..." name="Passport">
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        <input type="number" placeholder="Type Numbre place..." name="place_reservé">
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="submit_btn">
                                        <button class="boxed-btn4" type="submit">Submit</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="bordered_1px"></div>
                </div>
            </div>
        </div>
    </div>

    <footer class="footer">
